{
	"repositories": [
		{
			"type": "composer",
			"url": "https://wpackagist.org",
			"only": [
				"wpackagist-plugin/*",
				"wpackagist-theme/*"
			]
		}
	],
	"require": {
		"nesbot/carbon": "^3.0",
		"wpackagist-plugin/akismet": "dev-trunk",
		"wpackagist-plugin/wordpress-seo": ">=7.0.2",
		"wpackagist-theme/hueman": "*"
	},
	"config": {
		"allow-plugins": {
			"composer/installers": true
		}
	}
}
